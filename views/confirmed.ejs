<!doctype html>
<html>

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/css/style.css?v=2.2">
    <link rel="icon" href="/images/webp/mei_logo_web.webp" type="image/x-icon" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://code.jquery.com/jquery-3.7.1.slim.min.js" integrity="sha256-kmHvs0B+OpCW5GVHUNjv9rOmY0IvSIRcf7zGUDTDQM8=" crossorigin="anonymous"></script>
    <title>Personal</title>
</head>

<body>
    <div class="nav" id="nav">
        <ul class="navbar" id="navbar">
            <li class="links"><a class="logosredes" href="/"> 
                <img class="wsplogo" src="/images/webp/home.webp" alt="home"></a></li>
            <li class="links2" id="logout"><a class="logosredes" href="/auth/logout"> 
                <img class="wsplogo" src="/images/webp/logout.webp" alt="logout"></a></li>
            <li class="links2 link3"><a class="pipe link">|</a></li>
            <li class="links2" id="cart"><a class="logosredes" href="/cart"> 
                <img class="wsplogo" src="/images/webp/cart.webp" alt="cart">
            </a></li>
            <li class="links2 link3" id="pipe"><a class="pipe link">|</a></li>
            <li class="links2 link3"><a class="translate link" key="USERNAME" href="/user/info">Tu info</a></li>
        </ul>
    </div>
    <div class="relleno2"></div>
    <div class="welcome">
        <p>Producto(s):</p>
        <% productos.forEach(producto => { %>
            <p><%= producto.productname %> x <%= producto.productquantity %></p>
        <% }) %>
        
        <p>Total pagado: <%= total %></p>
    </div>
    <script src="/js/hide.js" type="text/javascript"></script>
</body>

</html>